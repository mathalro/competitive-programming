#include <bits/stdc++.h>
#define maxn 1000

using namespace std;

struct edge {
	int u, v, c;
	edge() {}
	edge(_u, _v, _c) : u(_u), v(_v), c(_c) {}
};

vector<edge> ed;
vector<int> g[maxn]
int pais[maxn];


void addEdge(int _u, int _v, int _c) {
	g[u].push_back(edge.size());
	ed.push_back(edge(u, v, c));
	g[v].push_back(edge.size());
	ed.push_back(edge(v, u, 0));
}


